{
  "name": "cashivo",
  "version": "1.0.0",
  "engines": {
    "node": ">=20.0.0"
  },
  "description": "Financial budgeting application: ingest, categorize, and summarize transactions.",
  "main": "src/index.js",
  "scripts": {
    "lint": "eslint . --ext .js",
    "lint:ejs": "ejslint \"views/**/*.ejs\"",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "ingest": "node src/ingest.js",
    "categorize": "node src/categorize.js",
    "evaluate": "node evaluate.js",
    "summary": "node src/summary.js",
    "start": "npm run ingest && npm run categorize && npm run summary",
    "serve": "node src/server.js",
    "test": "jest --runInBand",
    "generate:categories": "node scripts/generate_categories.js",
    "train:classifier": "node scripts/train_classifier.js",
    "train:knn-classifier": "node scripts/train_knn_classifier.js",
    "prepare": "husky install || true"
  },
  "dependencies": {
    "@tensorflow-models/universal-sentence-encoder": "^1.3.3",
    "@tensorflow/tfjs-node": "^4.22.0",
    "hnswlib-node": "*",
    "@xenova/transformers": "^2.17.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "csurf": "^1.11.0",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.5.0",
    "ejs": "^3.1.10",
    "express": "^4.18.2",
    "helmet": "^8.1.0",
    "multer": "^1.4.5-lts.1",
    "openai": "^4.6.0",
    "p-limit": "^3.1.0"
    ,"@tensorflow/tfjs": "^4.22.0"
  },
  "devDependencies": {
    "ejs-lint": "^2.0.1",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.0",
    "jest": "^29.6.0",
    "lint-staged": "^16.0.0",
    "prettier": "^3.0.0",
    "supertest": "^6.3.3"
  },
  "overrides": {
    "csurf": {
      "cookie": "^0.7.2"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run lint -- --fix",
      "npm run format"
    ],
    "*.ejs": [
      "npm run lint:ejs"
    ]
  }
}
